{
  "name": "mofb-api",
  "version": "1.0.0",
  "description": "RESTful MOFB API",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "test:integration": "nyc mocha \"test/integration/**/*.js\" --exit",
    "test": "nyc --reporter=html --reporter=text mocha \"test/unit/**/*.js\" --exit",
    "build": "npm install && npm run test",
    "lint": "eslint './**/*.js'",
    "lint:fix": "eslint --fix './**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SCODEMeetup/mofb-api.git"
  },
  "keywords": [
    "RESTful"
  ],
  "author": "SCODE-API",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SCODEMeetup/mofb-api/issues"
  },
  "homepage": "https://github.com/SCODEMeetup/mofb-api#readme",
  "nodemonConfig": {
    "restartable": "rs",
    "verbose": true,
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "Postman Collection/"
    ],
    "watch": [
      "server.js",
      "api/",
      "swagger.yaml"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "csv": "^5.1.1",
    "express": "^4.16.3",
    "lodash": "^4.17.14",
    "md5": "^2.2.1",
    "node-cache": "^4.2.0",
    "nodemon": "^1.18.4",
    "request": "^2.88.0",
    "sinon": "^7.2.6",
    "swagger-ui-express": "^4.0.1",
    "yamljs": "^0.3.0"
  }
}
